FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  RAS; // Reynolds-Averaged Simulation

RAS
{
    RASModel        kEpsilon;  // Mod√®le k-epsilon standard
    turbulence      on;
    printCoeffs     on;
}
